@{
    ViewBag.Title = "OpsTest";
}

<link href="~/css/ops/pages/ops.css?v=1.1" rel="stylesheet" />

<style>
    #mdlShowMachine {
       @* z-index: 999;*@ /*HA ADD*/
    }
    .row {
        margin-left: 0px;
        margin-right: 0px;
    }
</style>

<div>
    <div class="row">
        <div class="col-sm-12">
            @*<h2>Ops Registration
                @Html.TextBox("text")
                </h2>*@
            <div id="dvOpsRegistration">
                @Html.Partial("~/Views/Menu/MenuPartialView.cshtml")
                <button type="button" id="btnDisableOp" class="btn-menu" style="display: none">
                    <span class="glyphicon glyphicon-remove-sign"></span> Disable
                </button>
            </div>
        </div>
    </div>

    <!--Key Style key code-->
    <input id="hdStyleCode" type="hidden" />
    <input id="hdStyleSize" type="hidden" />
    <input id="hdStyleColor" type="hidden" />
    <input id="hdStyleRevNo" type="hidden" />
    <input id="hdOpsLanguage" type="hidden" />
    <!--Keep status confirm-->
    <input id="hdConfChkOpsMaster" type="hidden" />

    <!--Keep max operation plan revision for add new ops-->
    <input id="hdOpRevNoMax" type="hidden" />

    <!--Keep Operation plan Revision no for Ops master (click row on Grid view Ops master)-->
    <input id="hdOpRevNo" type="hidden" />
    <input id="hdOpSerial" type="hidden" />

    <!-- Modal register new operation plan -->
    <div class="modal fade modal-custom" id="opsModal" role="dialog" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Copy / Register new operation plan</h4>
                </div>
                <div class="modal-body">
                    <div class="modal-body-custom">
                        <div class="modal-content-ops">
                            <div class="row">
                                <div class="col-sm-4 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label no-padding-right"> Buyer </label>
                                            <div class="col-sm-5 selec2-custom">
                                                <select id="drpBuyer" class="form-control" data-placeholder="Select Buyer"></select>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="col-sm-4 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-4 control-label no-padding-right" for="txtAoNumber"> Ao Number </label>
                                            <div class="col-sm-5">
                                                <input type="text" id="txtAoNumber" class="form-control">
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="col-sm-4 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label no-padding-right" for="txtStyleFilter"> Style </label>
                                            <div class="col-sm-5">
                                                <input type="text" id="txtStyleFilter" class="form-control">
                                            </div>
                                            <input id="btnFilter" class="btn btn-primary btn-modal" type="button" value="Search" />
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div class="modal-content-ops">
                            <div class="row ">
                                <div class="col-sm-6 col-xs-12">
                                    <div class="col-xs-12">
                                        <!-- JQGRID STYLE BEGINS -->
                                        <div class="">
                                            <table id="gridTableStyleModal"></table>
                                            <div id="gridPaperStyleModal"></div>
                                        </div>
                                        <!-- JQGRID STYLE ENDS -->
                                    </div><!-- /.col -->
                                </div>
                                <div class="col-sm-6 col-xs-12">
                                    <div class="col-xs-12">
                                        <!-- JQGRID OPERATION DETAIL BEGINS -->
                                        <div class="">
                                            <table id="gridTableOpsModal"></table>
                                            <div id="gridPagerOpsModal"></div>
                                        </div>
                                        <!-- JQGRID OPERATION DETAIL ENDS -->
                                    </div><!-- /.col -->
                                </div>
                            </div>
                        </div>
                        <div class="modal-content-ops">
                            <div class="row ">
                                <div class="col-sm-4 col-xs-12">
                                    <div class="checkbox">
                                        <label><input type="checkbox" id="chkRegisterNeutralColor" value="">Register as neutral color</label>
                                    </div>
                                    <div class="checkbox">
                                        <label><input type="checkbox" id="chkCopyBOMPattern" value="">Copy BOM and Patterns</label>
                                    </div>
                                </div>
                                <div class="col-sm-4 col-xs-12">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="checkbox">
                                                <label><input type="checkbox" id="chkCopyToolLinking" checked="checked">Copy tool, machine linking</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <select name="countries" class="form-control pointer" id="drpLanguageOpMaster">
                                                <option value="vn" data-image="/assets/country-dropdown/images/msdropdown/icons/blank.gif" data-imagecss="flag flag-vn" data-title="Việt Nam">Tiếng Việt</option>
                                                <option value="gb" data-image="/assets/country-dropdown/images/msdropdown/icons/blank.gif" data-imagecss="flag flag-gb" data-title="English">English</option>
                                                <option value="id" data-image="/assets/country-dropdown/images/msdropdown/icons/blank.gif" data-imagecss="flag flag-id" data-title="Indonesia">Bahasa</option>
                                                <option value="mm" data-image="/assets/country-dropdown/images/msdropdown/icons/blank.gif" data-imagecss="flag flag-mm" data-title="မြန်မာဘာသာ">မြန်မာဘာသာ</option>
                                                <option value="et" data-image="/assets/country-dropdown/images/msdropdown/icons/blank.gif" data-imagecss="flag flag-et" data-title="Amharic">Amharic</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <input id="btnTranslateProcessName" type="button" class="btn btn-info form-control btn-modal" value="Translate" style="margin-top:2px; display:none" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4 col-xs-12">
                                    <div class="radio">
                                        <label><input type="radio" id="rdCopySelectPlan" name="registerPlan" checked>Copy the selected plan</label>
                                    </div>
                                    <div class="radio">
                                        <label><input type="radio" id="rdRegisterEmptyPlan" name="registerPlan">Register an empty plan</label>
                                    </div>
                                    <div class="radio">
                                        <label><input type="radio" id="rdImportCsv" name="registerPlan">Import from file (GSD)</label>
                                        <input id="btnUploadCsv" type="button" class="btn btn-success btn-modal" disabled value="CSV..." />
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="modal-content-ops">
                            <div class="row">
                                <div class="col-sm-2 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtStyleCode"> Style Code </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtStyleCode" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtStyleSize"> Style Size </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtStyleSize" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                    </form>

                                </div>
                                <div class="col-sm-2 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtColor"> Color </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtColor" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtRevNo"> Revision </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtRevNo" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="col-sm-2 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtProcess"> Processes </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtProcess" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtMachine"> Machines </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtMachine" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="col-sm-2 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtWorkerModal"> Workers </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtWorkerModal" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtTime"> Time </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtTime" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                                <div class="col-sm-2 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right" for="txtOpRevision"> OpRev </label>
                                            <div class="col-sm-6">
                                                <input type="text" id="txtOpRevision" placeholder="" class="form-control" readonly>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-6 control-label no-padding-right"> Target edition </label>

                                            <div class="col-sm-6 selec2-custom">
                                                <select id="drpTargetEdition" class="form-control" data-placeholder="Select edition"></select>
                                            </div>

                                        </div>
                                    </form>
                                </div>
                                @*HA Start Mod*@
                                <div class="col-sm-2 col-xs-12">
                                    <form class="form-horizontal" role="form">
                                        <div class="form-group">
                                            @*<label class="col-sm-5 control-label no-padding-right"> Factory </label>*@

                                            <div class="col-sm-12 selec2-custom">
                                                <select id="drpFactoryOpmt" class="form-control" data-placeholder="Factory"></select>
                                            </div>

                                        </div>
                                        <div class="form-group">
                                            <div class="col-sm-6">
                                                <button type="button" id="btnRegisterOps" class="btn btn btn-primary btn-modal form-control">Register</button>
                                            </div>
                                            <div class="col-sm-6">
                                                <button type="button" class="btn btn btn-primary btn-modal form-control" data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </form>

                                </div>
                                @*HA SEndtart Mod*@
                            </div>
                        </div>
                    </div>
                </div>
                @*<div class="modal-footer">
                        <button type="button" id="btnRegisterOps" class="btn btn btn-primary btn-modal">Register</button>
                        <button type="button" class="btn btn btn-primary btn-modal" data-dismiss="modal">Close</button>
                    </div>*@
            </div>

        </div>
    </div>

    <!-- Modal import CSV file -->
    <div class="modal fade modal-custom" id="modalImportCsv" role="dialog" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Import operation plan</h4>
                </div>
                <div class="modal-body">
                    <div class="modal-body-custom">
                        @*<div class="modal-content-ops">*@
                        <div class="row">
                            <div class="col-sm-12 col-xs-12">

                                <form>
                                    <label class="radio-inline"><input type="radio" id="rdSelectProcessName" name="ProcessRadio">Select process name</label>
                                    <label class="radio-inline"><input type="radio" id="rdAddNewProcess" name="ProcessRadio">Add new processes</label>
                                </form>
                                <div class="space-4"></div>
                                <div id="dvImportFileError" class="error" style="display:none"></div>
                                @*<div id="dvOpsTableImport" class="jqgrid-container">*@
                                <div class="col-xs-12" style="padding-left:1px;">
                                    <table id="gridOpsTableImport"></table>
                                </div>
                                @*</div>*@
                            </div>
                        </div>
                        @*</div>*@
                    </div>
                </div>
                <div class="modal-footer">
                    <label class="btn btn btn-primary btn-modal">
                        <i class="glyphicon glyphicon-import"></i>
                        <span>Csv...</span>
                        <input type="file" id="flImportCsv" accept=".csv" style="display: none;">
                    </label>
                    <button type="button" id="btnAccept" class="btn btn btn-primary btn-modal">Accept</button>
                    <button type="button" class="btn btn btn-primary btn-modal" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal import CSV file -->
    <div class="modal fade modal-custom" id="mdlUpdateProcessTime" role="dialog" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Update process time</h4>
                </div>
                <div class="modal-body">
                    <div class="modal-body-custom">
                        <div class="row">
                            <div class="col-sm-12 col-xs-12">
                                <div id="divUpdateProcessTimeError" style="color:red"></div>
