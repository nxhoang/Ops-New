        function ChangLanguage(thing) {
            var curLang = $("#flagSelected").attr("value");
            var lang = $(thing).attr("value");
            if (curLang === lang) {
                return;
            }
            var hr = window.location.href;
            var url = "/ChangeLanguage/ChangeLanguage?culture=" + lang + "&returnUrl=" + hr;
            window.location.href = url;
            //Keep language id is local
            localStorage.setItem(LanguageId, lang);
        }
        function GetLangCookie() {
            var curLang = $("#flagSelected").attr("value");
            var lang = localStorage.getItem(LanguageId);
            if (curLang === lang) return;
            if (!$.isEmptyObject(lang)) {
                $("#liLanguage").find("a.list-group-item[value='" + lang + "']").trigger("click");
            }
        }
        function SetLanguage(thing) {
            var lang = $(thing).val();
            var hr = window.location.href;
            var url = "/ChangeLanguage/ChangeLanguage?culture=" + lang + "&returnUrl=" + hr;
            window.location.href = url;

            //Keep language id is local
            localStorage.setItem(LanguageId, lang);
        }

        function SetLang() {
            //START  MOD - SON - 2020.12.25) 25/Dec/2020
            //var curLang = 'HttpContext.Current.Request.Cookies["language"].Value';
            var curLang = '@HttpContext.Current.Request.Cookies["language"]';
            if (curLang == null || curLang.length === 0 || curLang === '') {
                curLang = 'en'
            }
            else {
                curLang = curLang.Value;
                @*curLang = '@HttpContext.Current.Request.Cookies["language"].Value';*@
            }
            //END  MOD - SON - 2020.12.25) 25/Dec/2020

            language = curLang;
            //$("#ddLanguage").val(curLang);
            var cl = "gb";
            switch (curLang) {
                case "df" || "en":
                    cl = "gb";
                    break;
                case "vi":
                    cl = "vn";
                    break;
                case "id":
                    cl = "id"
                    break;
                case "ko":
                    cl = "kr";
                    break;
                case "mm":
                    cl = "mm";
                    break;
                case "et":
                    cl = "am";
                    break;
                default:
                    cl = "gb";
                    break;
            }
            $("#flagSelected").attr("value", curLang);
            $("#flagSelected").find("span").addClass(cl);
            //$("#hflagSelected").find("span").addClass(cl);
        }

