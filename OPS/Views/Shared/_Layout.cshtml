    <script src="~/Scripts/jquery.blockUI.js"></script>
    <!--START SMSG Manager MESSAGE BOX-->
    <!--Author: VitHV-->
    <script src="~/js/ops/generic/SmSgManager.js"></script>
    <!--END SmSgManager-->
    <script>
        var language;
        $(document).bind("ajaxSend", function () {
            $.blockUI(window.BlockUI);
        }).bind("ajaxComplete", function () {
            $.unblockUI();
        });

        @*<script type="text/javascript" src="~/js/demo_dashboard.js"></script>*@

        $(document).ready(function () {

            //$("#ddLanguage").change(function () {
            //    SetLanguage(this);
            //});
            $("#liLanguage").find("a").click(function () {
                ChangLanguage(this);
            });

            $(document).ajaxError(function (xhr, props) {

                if (props.status === 401) {
                    location.reload();
                }

            });

            SetLang();
            GetLangCookie();
            //$("#ddLanguage").msDropdown();

            //START ADD - SON
            UpdateEmail();
            ShowModalEmailProcessVideo();
            SendEmailProcess();
            //END ADD - SON

            ReloadSearchStyle(); // HA ADD
            GetUserImage();

            //START ADD - SON) 3/Sep/2020
            EventClickButtonLayout();
            FillDataToDropdownlistLayout();
            BindDataToGridRecentPlan(null, null, null);
            //END ADD - SON) 3/Sep/2020

        });

        //==================================
        //START ADD: HA
        function ReloadSearchStyle() {
            var text = sessionStorage.getItem("searchStyleCode");
            if (text) {
                $("#searchStyle").val(text);
            }
        }

        function GetUserImage() {
            var src = 'http://203.113.151.204:8080/BETAPDM/User/';
            var substr = $("#hdUsername").val();
            var url = src + substr + '.jpg';

            var userInfo = GetUserInforByUserName(substr);
            var name = userInfo.usmt.Sex;

            CheckImageExists(url, function (existsImage) {
                if (existsImage == true) {
                    $("#userImage").attr("src", url);
                }
                else {
                    if (name == 'M') {
                        $("#userImage").attr("src", '/assets/jquery-comments/image/icons-male.jpg');
                    }
                    else if (name == 'F') {
                        $("#userImage").attr("src", '/assets/jquery-comments/image/icon-female.jpg');
                    }
                    else $("#userImage").attr("src", '/assets/jquery-comments/image/icons.jpg');
                }
            });
        }

        function CheckImageExists(imageUrl, callBack) {
            var imageData = new Image();
            imageData.onload = function () {
                callBack(true);
            };
            imageData.onerror = function () {
                callBack(false);
            };
            imageData.src = imageUrl;
        }

        function GetUserInforByUserName(userName) {
            var userInfo;
            $.ajax({
                url: '/Account/GetUserInforByUserName/',
                async: false,
                type: "POST",
                data: JSON.stringify({ userName: userName }),
                dataType: "json",
                contentType: "application/json",
                success: function (res) {
                    userInfo = res;
                },
                error: function (jqXhr, status, errorThrown) {
                    userInfo = null;
                }
            });
            return userInfo;
        }
        //END ADD: HA

        function ChangLanguage(thing) {
            var curLang = $("#flagSelected").attr("value");
            var lang = $(thing).attr("value");
            if (curLang === lang) {
                return;
            }
            var hr = window.location.href;
            var url = "/ChangeLanguage/ChangeLanguage?culture=" + lang + "&returnUrl=" + hr;
            window.location.href = url;
            //Keep language id is local
            localStorage.setItem(LanguageId, lang);
        }
        function GetLangCookie() {
            var curLang = $("#flagSelected").attr("value");
            var lang = localStorage.getItem(LanguageId);
            if (curLang === lang) return;
            if (!$.isEmptyObject(lang)) {
                $("#liLanguage").find("a.list-group-item[value='" + lang + "']").trigger("click");
            }
        }
        function SetLanguage(thing) {
            var lang = $(thing).val();
            var hr = window.location.href;
            var url = "/ChangeLanguage/ChangeLanguage?culture=" + lang + "&returnUrl=" + hr;
            window.location.href = url;

            //Keep language id is local
            localStorage.setItem(LanguageId, lang);
        }

        function SetLang() {
            var curLang = '@HttpContext.Current.Request.Cookies["language"].Value';
            language = curLang;
            //$("#ddLanguage").val(curLang);
            var cl = "gb";
            switch (curLang) {
                case "df" || "en":
                    cl = "gb";
                    break;
                case "vi":
                    cl = "vn";
                    break;
                case "id":
                    cl = "id"
                    break;
                case "ko":
                    cl = "kr";
                    break;
                case "mm":
                    cl = "mm";
                    break;
                case "et":
                    cl = "am";
                    break;
                default:
                    cl = "gb";
                    break;
            }
            $("#flagSelected").attr("value", curLang);
            $("#flagSelected").find("span").addClass(cl);
            //$("#hflagSelected").find("span").addClass(cl);
        }

        var BlockUI = {
            css: {
                border: 'none',
                padding: '15px',
                backgroundColor: '#000',
                '-webkit-border-radius': '10px',
                '-moz-border-radius': '10px',
                opacity: 0.5,
                color: '#fff'
            }
        }
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
